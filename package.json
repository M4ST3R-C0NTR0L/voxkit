{
  "name": "voxkit",
  "version": "1.0.0",
  "description": "Open source voice agent framework â€” build AI voice agents in ~20 lines of TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    }
  },
  "files": [
    "dist",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "voxkit": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "voice",
    "ai",
    "agent",
    "openai",
    "speech-to-text",
    "stt",
    "tts",
    "text-to-speech",
    "websocket",
    "realtime",
    "conversation",
    "framework",
    "gpt-4o",
    "claude",
    "grok",
    "deepgram",
    "voxkit"
  ],
  "author": "VoxKit Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voxkit/voxkit.git"
  },
  "bugs": {
    "url": "https://github.com/voxkit/voxkit/issues"
  },
  "homepage": "https://github.com/voxkit/voxkit#readme",
  "dependencies": {
    "ws": "^8.16.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.2.0",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "@types/node": "^20.11.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
